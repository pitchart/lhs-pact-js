{
  "consumer": {
    "name": "PRODUCT-WEB"
  },
  "interactions": [
    {
      "description": "Retrieve a product with a not existing id",
      "request": {
        "method": "GET",
        "path": "/products/19"
      },
      "response": {
        "status": 404
      }
    },
    {
      "description": "Retrieve a product with an id",
      "request": {
        "method": "GET",
        "path": "/products/1"
      },
      "response": {
        "body": {
          "id": "1",
          "name": "28Deg",
          "type": "credit_card"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
    {
      "description": "Retrieve all the products",
      "request": {
        "method": "GET",
        "path": "/products"
      },
      "response": {
        "body": [
          {
            "id": "1",
            "name": "28Deg",
            "type": "credit_card"
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "10.0.0-beta.62"
    },
    "pactRust": {
      "ffi": "0.2.4",
      "models": "0.3.1"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "PRODUCT-API"
  }
}